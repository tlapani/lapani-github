{
	"name": "ASQL_to_ASQLDataWarehouse",
	"properties": {
		"description": "Load data into Azure SQL Data Warehouse (Azure Synapse Analytics)",
		"activities": [
			{
				"name": "ASQLDB_to_ASQLDataWarehouse",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "lego.themes"
					},
					{
						"name": "Destination",
						"value": "."
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						},
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ASQLDB_Lego_Themes",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}