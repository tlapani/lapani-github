{
	"name": "OnPremise_Emp_SQL_to_ADLS",
	"properties": {
		"description": "Copying EMP table data from SQL server on premise database to ADLS",
		"activities": [
			{
				"name": "CopyFromOnPremiseSqlToAzureBlob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.emp"
					},
					{
						"name": "Destination",
						"value": "sqltableasfile//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "empno",
									"type": "Int32"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 1
								}
							},
							{
								"source": {
									"name": "ename",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 2
								}
							},
							{
								"source": {
									"name": "job",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 3
								}
							},
							{
								"source": {
									"name": "mgr",
									"type": "Int32"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 4
								}
							},
							{
								"source": {
									"name": "hiredate",
									"type": "DateTime"
								},
								"sink": {
									"type": "DateTime",
									"ordinal": 5
								}
							},
							{
								"source": {
									"name": "sal",
									"type": "Int32"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 6
								}
							},
							{
								"source": {
									"name": "comm",
									"type": "Int32"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 7
								}
							},
							{
								"source": {
									"name": "deptno",
									"type": "Int32"
								},
								"sink": {
									"type": "Int32",
									"ordinal": 8
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "OnPremiseSQLEmpTable",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLB_Emp_File",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}