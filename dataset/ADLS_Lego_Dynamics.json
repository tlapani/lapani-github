{
	"name": "ADLS_Lego_Dynamics",
	"properties": {
		"description": "Dynamic sink dataset to receive files from HTTP to ADSL",
		"linkedServiceName": {
			"referenceName": "ADLS_lapaniazuredatalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "String",
				"defaultValue": "FileName"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{dataset().FileName}.csv",
					"type": "Expression"
				},
				"fileSystem": "lego"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "id",
				"type": "String"
			},
			{
				"name": "name",
				"type": "String"
			},
			{
				"name": "parent_id",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}